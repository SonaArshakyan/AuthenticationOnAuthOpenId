@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>


<div class="container">
    <h1>Claims of User</h1>
   
    <div class="row">
        <div class="col-12">

            <table class=" table-bordered" style="width:100%">
                @foreach(var claim in User.Claims){
                <tr>
                    <td>@claim.Type</td>
                    <td>@claim.Value</td>

                </tr>
                }
                
            </table>    
        </div>
    </div>    

</div>
